{
	"type": "module",
	"name": "misskey-bubble-game",
	"version": "0.0.1",
	"types": "./built/dts/index.d.ts",
	"exports": {
		".": {
			"import": "./built/esm/index.js",
			"types": "./built/dts/index.d.ts"
		},
		"./*": {
			"import": "./built/esm/*",
			"types": "./built/dts/*"
		}
	},
	"scripts": {
		"build": "node ./build.js",
		"build:tsc": "pnpm tsc-esm && pnpm tsc-dts",
		"tsc-esm": "tsc --outDir built/esm",
		"tsc-dts": "tsc --outDir built/dts --declaration true --emitDeclarationOnly true --declarationMap true",
		"watch": "nodemon -w src -e ts,js,cjs,mjs,json --exec \"pnpm run build:tsc\"",
		"typecheck": "tsc --noEmit",
		"biome": "biome",
		"format": "biome check --write",
		"lint": "pnpm typecheck && biome check"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/matter-js": "0.19.7",
		"@types/node": "22.9.1",
		"@types/seedrandom": "3.0.8",
		"nodemon": "3.1.7",
		"typescript": "5.6.3"
	},
	"files": ["built"],
	"dependencies": {
		"esbuild": "0.24.0",
		"eventemitter3": "5.0.1",
		"glob": "11.0.0",
		"matter-js": "0.20.0",
		"seedrandom": "3.0.5"
	}
}
